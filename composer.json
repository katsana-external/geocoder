{
    "name": "katsana/geocoder",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\Geocoder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katsana\\Geocoder\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "orchestra/support": "^3.8 || ^4.0",
        "willdurand/geocoder": "^4.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8 || ^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
